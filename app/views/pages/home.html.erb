        <input type="checkbox" id="Blue" name="Blue"checked>
        <label for="scales">Blue</label>
        <input type="checkbox" id="Gren" name="Green">
        <label for="XL">Green</label>
        <input type="checkbox" id="Brown" name="Brown">
        <label for="XXL">Brown</label>
  </div>
  <div <b>Size: </b>
        <input type="checkbox" id="s" name="s"checked>
        <label for="s">S</label>
        <input type="checkbox" id="m" name="m">
        <label for="m">M</label>
        <input type="checkbox" id="L" name="L"checked>
